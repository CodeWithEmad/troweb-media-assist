{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Streamlit: Home",
            "type": "debugpy",
            "request": "launch",
            "module": "streamlit",
            "args": [
                "run",
                "Home.py",
                "--server.port",
                "8501"
            ],
            "justMyCode": true,
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        }
    ]
}
